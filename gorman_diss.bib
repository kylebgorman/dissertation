@article{Hill1954,
    Annote = {444f.: Sequences of more than two vowels always contain one or more vowel extremes. In a three-vowel sequence, if the second is an extreme it assumes its consonantal shape, with syllable division before it: \emph{Auernus} /a-uernus/; the rule applies even if the first and third are also extremes: \emph{biiugis} `yoked two together' /bi-iugis/. If the second is a non-extreme vowel, first and third assume their consonantal shape without syllable division: \emph{uae} /uai/. In a four-vowel sequnce, the treatment depends primarily on the first vowel. If it is non-extreme, consonantal allophones appear for the second and third, with division between them: \emph{aeuī} /ai-uiL/ and (in spite of the spelling) \emph{maius} /mai-ius/. If the first is an extreme, consonantal allophones appear for the first and third, with division before the third: \emph{iuuō} /iu-uoL/.

One other type of vowel sequence involves no necessary juncture: a long vowel followed by another vowel. This type is always dissyllabic, the /L/ closing the first syllable as we should expect: \emph{āeris} /aL-eris/ `of the air', and \emph{dēerrō} /deL-erroL/ `I lose my way'. 

Other apparent vowel sequences contain a juncture, and contrast with those which do not. A good minimal contrast is that between \emph{aes} `brass', and \emph{ais} `thou sayest'. Phonetically the two are [áys] and [áˑǐs], the first one being one syllable and containing only one true vowel, the second being two syllables separated by a half-unit of lengthening and containing two vowels. Phonemically, the two are /ais/ and /a+is/. Other examples of sequences with juncture are \emph{deinde} /de+inde/ `thereupon', \emph{coit} /ko+it/ `it comes together', and \emph{ruit} /ru+it/ `it falls'; the rule is that two true vowels are always separated by a consonant, by length, or by a juncture.},
    Author = {Archibald A. Hill},
    Title = {Juncture and syllable division in {Latin}},
    Journal = {Language},
    Volume = {30},
    Number = {?},
    Pages = {439-447},
    Year = {1954}}

